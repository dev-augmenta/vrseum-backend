{
	"local": {
		"provider": "local",
		"module": "passport-local",
		"usernameField": "email",
		"passwordField": "password",
		"authPath": "/auth/local",
		"successRedirect": "/auth/account",
		"failureRedirect": "/local",
		"failureFlash": false,
		"setAccessToken" : true,
		"session" : true
	}
}
